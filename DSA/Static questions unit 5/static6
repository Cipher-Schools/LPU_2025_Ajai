#include <stdio.h>

int table[10];

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 0; i < 10; i++)
        table[i] = -1;

    int max = -1;
    int maxIndex = -1;

    for (int i = 0; i < n; i++) {
        int key;
        scanf("%d", &key);

        int index = key % 10;
        int j = 0;

        while (table[(index + j*j) % 10] != -1)
            j++;

        int pos = (index + j*j) % 10;
        table[pos] = key;

        if (key > max) {
            max = key;
            maxIndex = pos;
        }
    }

    printf("The lorry that has high load capacity is in index %d.", maxIndex);
    return 0;
}